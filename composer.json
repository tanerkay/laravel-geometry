{
    "name": "spinen/laravel-geometry",
    "description": "Wrapper over the geoPHP Class to make it integrate with Laravel better.",
    "keywords": [
        "geometry",
        "geoPHP",
        "laravel",
        "library",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Thai-Son Le",
            "email": "thaison.le@spinen.com"
        },
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/contracts": "^5.5|^6.0|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0",
        "phayes/geophp": "~1.2"
    },
    "require-dev": {
        "mockery/mockery": "^1",
        "phpunit/phpunit": "~7.0.1|~8.0",
        "psy/psysh": "^0.9.0",
        "symfony/thanks": "^1.1",
        "symfony/var-dumper": "~3.0|^4.2"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\Geometry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\Geometry\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\Geometry\\GeometryServiceProvider"
            ],
            "aliases": {
                "Geo": "Spinen\\Geometry\\GeometryFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
